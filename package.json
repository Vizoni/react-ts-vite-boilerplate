{
  "name": "react-ts-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint-check": "eslint ./src --c .js,.ts,.tsx  -c eslint.config.js --max-warnings 10 -f pretty",
    "lint-fix": "eslint ./src --c .js,.ts,.tsx  -c eslint.config.js --max-warnings 10 -f pretty --fix",
    "lint-check:report": "eslint ./src --c .js,.ts,.tsx -o ./reports/eslint-report/index.txt -c eslint.config.js --max-warnings 10 -f pretty",
    "prettier-check": "prettier --config ./.prettierrc --check \"./src\"",
    "prettier-fix": "prettier --config ./.prettierrc --check \"./src\" --write",
    "preview": "vite preview",
    "tsc: watch": "tsc -b -w",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "test": "jest --coverage=false --passWithNoTests",
    "test:coverage": "jest --passWithNoTests",
    "test:watch": "jest --coverage=false --watch",
    "tsc:watch": "tsc -w -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "history": "^5.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.3",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "14.6.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "eslint": "8.57.1",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-axios": "4.8.0",
    "jest-sonar-reporter": "2.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3",
    "vite-plugin-simple-html": "^0.2.0"
  },
  "resolutions": {
    "jest": "^29.7.0"
  }
}
